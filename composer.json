	"description": "Code source file forensics utility",
	"minimum-stability": "stable",
	"license": "proprietary",
	"authors": [
		{
			"name": "reid",
			"email": "reid@rawsound.com"
		}
	],
	"require": {
		"php": "^7.4",
		"ext-intl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-phalcon": "^4.1",
		"corneltek/getoptionkit": "^2.6",
		"diskerror/typed": "dev-master",
		"laminas/laminas-server": "^2.10",
		"php-ds/php-ds": "^1.3"
	},
	"suggest": {
		"ext-ds": "Compiled version of \"php-ds\"."
	},
	"require-dev": {
		"phalcon/ide-stubs": "^4.1"
	},
	"autoload": {
		"classmap": ["tasks"],
		"psr-4": {
			"Logic\\": "logic",
			"Resource\\": "resource",
			"Service\\": "service",
			"Structure\\": "struct"
		},
		"files": ["service/StdIo.php"]
